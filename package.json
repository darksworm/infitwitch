{
  "name": "infitwitch",
  "version": "1.0.0",
  "description": "infitwitch",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack -w",
    "compress": "rm -f ./dist.zip && rm -f ./dist/js/styles.js && cd ./dist && zip -r ../dist.zip .",
    "compress-source": "rm -f ./source.zip && zip -r ./source.zip . -x .\\* -x \\*.pem -x \\*.zip -x node_modules/\\* -x dist/\\*",
    "builds": "npm run compress | npm run compress-source"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/chibat/chrome-extension-typescript-starter.git"
  },
  "dependencies": {
    "jquery": "~3.2.1",
    "jquery-sortable": "^0.9.13"
  },
  "devDependencies": {
    "@types/chrome": "~0.0.47",
    "@types/jquery": "^3.2.5",
    "@types/jquery-sortable": "^0.9.29",
    "babili-webpack-plugin": "^0.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "strip-loader": "^0.1.2",
    "svg-url-loader": "^2.0.2",
    "ts-loader": "~2.2.0",
    "typescript": "~2.3.4",
    "webpack": "~3.0.0"
  }
}
